# Copy this file to .env and fill in your actual values
# This file contains sensitive credentials for code signing and notarization

# Developer ID for code signing (optional, script uses automatic signing by default)
IDENTITY="Developer ID Application: xxxx (XXXXXXXXXX)"

# Apple ID email for notarization
APPLE_ID="your-apple-id@example.com"

# App-specific password for notarization
# Generate at: https://appleid.apple.com/account/manage
APP_PASSWORD="xxxx-xxxx-xxxx-xxxx"

# Team ID (found in Apple Developer portal)
TEAM_ID="XXXXXXXXXX"

# App configuration
APP_NAME="MicOver"
BUNDLE_ID="com.yourcompany.micover"